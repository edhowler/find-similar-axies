{
    "manifest_version": 3,
    "name": "Find Similar Axies Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://app.axieinfinity.com/marketplace/axies/*"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ],
    "permissions": []
}